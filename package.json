{
  "name": "artificial",
  "displayName": "artificial",
  "description": "Does things",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:stubs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsWelcome": [
      {
        "view": "stubGenerate",
        "contents": "[Generate Stubs](command:stubs.generateStubs)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "artificial-workflows",
          "title": "Artificial Workflows",
          "icon": "media/artificial.svg"
        }
      ]
    },
    "views": {
      "artificial-workflows": [
        {
          "id": "stubs",
          "name": "Functions",
          "icon": "media/dep.svg"
        },
        {
          "id": "stubGenerate",
          "name": "Generate Stubs"
        }
      ]
    },
    "commands": [
      {
        "command": "stubs.generateStubs",
        "title": "Generate Stubs",
        "category": "Artificial"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "dt-python-parser": "^0.9.0"
  }
}
